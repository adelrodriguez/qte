{
  "name": "qte",
  "version": "0.1.0",
  "description": "Parse human-readable time expressions into any unit, and format durations into human-readable strings",
  "keywords": [
    "convert",
    "duration",
    "format",
    "human-readable",
    "milliseconds",
    "ms",
    "parse",
    "time"
  ],
  "homepage": "https://github.com/adelrodriguez/qte#readme",
  "bugs": {
    "url": "https://github.com/adelrodriguez/qte/issues"
  },
  "license": "MIT",
  "author": "Adel Rodr√≠guez",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adelrodriguez/qte.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": false,
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "bunup",
    "build:debug": "bunup --sourcemap inline --no-minify",
    "bump:deps": "bun update --interactive",
    "check": "adamantite check",
    "dev": "bunup --watch --sourcemap inline --no-minify",
    "fix": "adamantite fix",
    "release": "changeset publish",
    "init": "bun run --bun scripts/init.ts",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "test:watch": "bun test --watch",
    "typecheck": "adamantite typecheck",
    "version": "changeset version && bun run format",
    "format": "adamantite format",
    "analyze": "adamantite analyze"
  },
  "devDependencies": {
    "@changesets/cli": "2.29.8",
    "@clack/prompts": "0.11.0",
    "@types/bun": "1.3.5",
    "@typescript/native-preview": "7.0.0-dev.20260108.1",
    "adamantite": "^0.25.0",
    "bunup": "0.16.17",
    "knip": "5.80.1",
    "oxfmt": "0.23.0",
    "oxlint": "1.38.0",
    "oxlint-tsgolint": "0.10.1"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "packageManager": "bun@1.3.1"
}
